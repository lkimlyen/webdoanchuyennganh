function Fillter(n){var t="";if(n.indexOf("?")<0)$(".listfilternomal").each(function(){var n=$(this),i=n.find("li.active").children();isEmty(i.data("value"))||(t+="&"+n.data("query")+"="+i.data("value"))}),t=isEmty(t)?n:n+"?"+t.substr(1,t.length-1);else{var i="",r=n.split("?")[0],u=$.makeArray(n.substring(n.indexOf("?")+1).split("&"));$(".listfilternomal").each(function(){var n=$(this),r=n.find("li.active").children();i+=n.data("query");isEmty(r.data("value"))||(t+="&"+n.data("query")+"="+r.data("value"))});$.each(u,function(n,r){var u=r.split("=");!isEmty(i)&&i.indexOf(u[0])<0&&r.indexOf("sl")<0&&(t+="&"+r)});t=isEmty(t)?r:r+"?"+t.substr(1,t.length-1)}return t}function FillterAdvance(n){var t="";if(n.indexOf("?")<0)$(".listfilteradvance").each(function(){var n=$(this),i=n.find("input[type='radio']:checked");isEmty(i.data("value"))||(t+="&"+n.data("query")+"="+i.data("value"))}),t=isEmty(t)?n:n+"?"+t.substr(1,t.length-1);else{var i="",r=n.split("?")[0],u=$.makeArray(n.substring(n.indexOf("?")+1).split("&"));$(".listfilteradvance").each(function(){var n=$(this),r=n.find("input[type='radio']:checked");i+=n.data("query");isEmty(r.data("value"))||(t+="&"+n.data("query")+"="+r.data("value"))});$.each(u,function(n,r){var u=r.split("=");!isEmty(i)&&i.indexOf(u[0])<0&&r.indexOf("sl")<0&&(t+="&"+r)});t=isEmty(t)?r:r+"?"+t.substr(1,t.length-1)}return t}function FillterMobile(n){var t="";if(n.indexOf("?")<0)$(".mfpt-filter-ullv").each(function(){var n=$(this),i=n.find("input[type='radio']:checked");isEmty(i.data("value"))||(t+="&"+n.data("query")+"="+i.data("value"))}),t=isEmty(t)?n:n+"?"+t.substr(1,t.length-1);else{var i="",r=n.split("?")[0],u=$.makeArray(n.substring(n.indexOf("?")+1).split("&"));$(".mfpt-filter-ullv").each(function(){var n=$(this),r=n.find("input[type='radio']:checked");i+=n.data("query");isEmty(r.data("value"))||(t+="&"+n.data("query")+"="+r.data("value"))});$.each(u,function(n,r){var u=r.split("=");!isEmty(i)&&i.indexOf(u[0])<0&&(t+="&"+r)});t=isEmty(t)?r: