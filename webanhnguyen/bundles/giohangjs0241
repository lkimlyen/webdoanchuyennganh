function ConvertStringToNameAscii(n,t){return n=n.toLowerCase().trim(),n=n.replace(/à|á|ạ|ả|ã|â|ầ|ấ|ậ|ẩ|ẫ|ă|ằ|ắ|ặ|ẳ|ẵ/g,"a"),n=n.replace(/è|é|ẹ|ẻ|ẽ|ê|ề|ế|ệ|ể|ễ/g,"e"),n=n.replace(/ì|í|ị|ỉ|ĩ/g,"i"),n=n.replace(/ò|ó|ọ|ỏ|õ|ô|ồ|ố|ộ|ổ|ỗ|ơ|ờ|ớ|ợ|ở|ỡ/g,"o"),n=n.replace(/ù|ú|ụ|ủ|ũ|ư|ừ|ứ|ự|ử|ữ/g,"u"),n=n.replace(/ỳ|ý|ỵ|ỷ|ỹ/g,"y"),n=n.replace(/đ/g,"d"),n=n.replace(/!|@|%|\^|\*|\(|\)|\+|\=|\<|\>|\?|\/|,|\.|\:|\;|\'|\"|\&|\#|\[|\]|~|$|_|–|”|“|`/g,"-"),n=t?n.replace(/ /g,""):n.replace(/ /g,"-"),n=n.replace(/-+-/g,"-"),n.replace(/^\-+|\-+$/g,"")}function endsWith(n,t){return n.indexOf(t,n.length-t.length)!==-1}function ConvertCurrency(n){return n?n.replace(/(\d)(?=(\d{3})+(?!\d))/g,"$1."):""}function CaculatorPriceCart(){var h=$(".listproincart"),n=0,r=0,f=1,e="",u=0,o=0,t=0,s,i;$.each(h,function(t,i){var c=$(i).attr("data-price"),l=$(i).find(".quantity-product"),h=parseInt($(l).val()),s;n=n+parseInt(c)*h;r=r+1;e=$($(i).find(".color-product option:selected")).text();f=h;s=$(".quantity-bhv option:selected").val();s>h&&($(".quantity-bhv").val(1),s=1);$("#bhv_check").is(":checked")?($(".div-quantity-bhv").show(),$(".quantity-bhv option").each(function(n,t){$(t).val()>h?$(t).attr("disabled",!0):$(t).attr("disabled",!1)}),u+=parseInt($("#bhv_check").attr("data-pricebhv"))*parseInt(s),o+=parseInt($("#bhv_check").attr("data-pricediscount"))*parseInt(s)):$(".div-quantity-bhv").hide()});$("#cartprice").text(ConvertCurrency(n.toString()));$("#bhv_check").is(":checked")?(n=n+u,$("#priceonline").text(ConvertCurrency((Math.round(n/2e4,0)*1e3>1e5?Math.round(n/2e4,0)*1e3:1e5).toString())),$("#txtListProductInCart").attr("data-priceonline",Math.round(n/2e4,0)*1e3>1e5?Math.round(n/2e4,0)*1e3:1e5),$("#payTotalOrder").text(ConvertCurrency((Math.round(n/1e3,0)*1e3).toString())),$("#totalcartprice").text(ConvertCurrency((Math.round(n/1e3,0)*1e3).toString())),$("#totalcartprice").attr("data-price",n),$("#pricebhv").text(ConvertCurrency(u.toString())),$("#lipricebhv").show()):($("#priceonline").text(Co