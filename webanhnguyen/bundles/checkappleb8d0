jfpts.checkapple=function(){function n(){var n=$('input[name="txtImei"]').val();if(n==""){jfpts.utility.notifyWarningCenter("Bạn vui lòng nhập imei sản phẩm.");return}jfpts.utility.showLoading();$.get("/Ajax/Apple/GetInfoApple",{imei:n},function(n){$("#imeierror").length&&$("#imeierror").remove();typeof n!="undefined"&&n!=""?n.check?($(".fck-cap").hide(),$(".ctn-cap").html(n.content).show()):$("#frmImei").after("<label for='FullName' id='imeierror' style='color:red;margin-top:5px;' class='error'>IMEI bạn nhập không chính xác, vui lòng nhập lại<\/label>"):jfpts.utility.notifyWarning("Xin lỗi, có lỗi xảy ra bạn vui lòng thao tác lại.")}).complete(function(){jfpts.utility.hideLoading()})}function t(){$('input[name="txtImei"]').val("");$(".fck-cap").show();$(".ctn-cap").html("")}function i(){$(".fshop-imei-form").submit(function(t){t.preventDefault();n()})}return{init:i,checkOtherImei:t}}();$(document).ready(function(){jfpts.checkapple.init()})